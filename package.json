{
  "name": "coviu-demo-plugin",
  "version": "1.0.0",
  "description": "Coviu Demo Plugin",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  },
  "scripts": {
    "dev-client": "npm run webpack --hot --inline",
    "build": "webpack -p --progress",
    "serve": "npm run build && http-server dist/ -p 9100 --cors",
    "publish": "npm run build && gulp publish",
    "webpack": "envade NODE_ENV=development -- webpack-dev-server --port=9100 --host=0.0.0.0 --colors --content-base dist"
  },
  "keywords": [],
  "author": "Coviu",
  "license": "Private",
  "dependencies": {
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "envade": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-awspublish": "3.0.0",
    "gulp-rename": "^1.2.2",
    "http-server": "^0.12.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.12.0",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3"
  }
}
